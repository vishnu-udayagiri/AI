    items="{path :'FilterDatamodel>/iata/',sorter: {path:'iata',descending: true}}"
     search="handleValueHelpSearchiataNum" multiSelect="true" rememberSelections="true"
      confirm="handleValueHelpCloseiataNum" cancel="handleValueHelpCloseiataNum">
